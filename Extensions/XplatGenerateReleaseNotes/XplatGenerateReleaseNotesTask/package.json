{
  "name": "XPlatGenerateReleaseNotesTask",
  "scripts": {
    "build": "npm run clean && npm run lint && npm run transpile && npm run copytestconsole",
    "lint": "tslint -c tslint.json src/*.ts test/*.ts testconsole/*.ts",
    "package": "npm prune -production && npm run copy",
    "copy": "ncp ./node_modules ./task/node_modules && ncp ./dist/src ./task",
    "copytestconsole": "ncp ./dist/src ./testconsole",
    "clean": "rimraf ./dist/src && rimraf ./dist/test && rimraf ./task/*.js && rimraf ./task/*.js.map && rimraf ./task/node_modules",
    "transpile": "tsc -p ./",
    "test": "mocha -r ts-node/register ./test/*.test.ts --reporter mocha-junit-reporter --reporter-options mochaFile=./test-output/test-results.xml ",
    "test-no-logger": "mocha -r ts-node/register ./test/*.test.ts "
    
  },
  "description": "XPlatGenerateReleaseNotesTask",
  "main": "GenerateReleaseNotes.js",
  "dependencies": {
    "@types/q": "^1.5.4",
    "@types/xmldom": "^0.1.30",
    "azure-devops-node-api": "^10.2.1",
    "azure-pipelines-task-lib": "^4.1.0",
    "fs": "^0.0.1-security",
    "handlebars": "^4.7.7",
    "handlebars-helpers": "^0.9.8",
    "minimist": "^1.2.6",
    "ncp": "^2.0.0",
    "request": "^2.88.2",
    "typed-rest-client": "^1.8.3"
  },
  "devDependencies": {
    "@types/chai": "^4.2.15",
    "@types/mocha": "^8.2.2",
    "@types/node": "^17.0.13",
    "chai": "^4.3.4",
    "del": "^6.0.0",
    "mocha": "^9.2.2",
    "mocha-junit-reporter": "^2.0.0",
    "tslint": "^5.20.1",
    "typescript": "^3.5.3",
    "ts-node": "^3.3.0"
  },
  "author": "Richard Fennell",
  "license": "MIT"
}
